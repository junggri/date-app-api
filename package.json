  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node --max-old-space-size=4096 dist/src/main",
    "start:dev": "nest start --watch --exec 'dotenv -e .env.development node'",
    "start:debug": "nest start --debug --watch",
    "start:local": "nest start --watch --exec 'dotenv -e .env.development node'",
    "start:prod": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nest start --watch --exec 'dotenv -e .env.production node'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch --verbose --detectOpenHandles ",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ormconfig.ts migration:generate -n blog",
    "migration:run": "typeorm --config dist/ormconfig.js migration:run",
    "migration:revert": "typeorm migration:revert"
  },
